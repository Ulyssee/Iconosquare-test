{
  "compilerOptions": {
    "allowJs": true, 
    "moduleResolution": "Node",
    "baseUrl": "./src",
    "paths": {
      "@components/*": ["components/*"],
      "@utils/*": ["utils/*"]
    },
    "jsx": "react-jsx",
    "esModuleInterop": true,
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules"],
}
